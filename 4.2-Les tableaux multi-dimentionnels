<?php
$indianaJonesMovies=[
  'Indiana Jones and the Raiders of the Lost Ark' => ['Harrison Ford,','Denholm Elliott,','John Rhys-Davies.'],
  'Indiana Jones and the Temple of Doom' => ['Harrison Ford,','Kate Capshaw,','Jonathan Ke Quan.'],
  'Indiana Jones and the Last Crusade' =>['Harrison Ford,','River Phoenix,','Denholm Elliott.',]
];

foreach($indianaJonesMovies as $movie=>$actor){
  echo 'Dans le film '. $movie.', les principaux acteurs sont: ';
  foreach($indianaJonesMovies[$movie] as $actor){
    echo $actor.' ';
  };
  echo"\n";
};
